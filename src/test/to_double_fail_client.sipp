  <recv response="100" dialog="1" use_txn="C1">
      <action>
         <ereg regexp="INVITE" search_in="hdr" header="CSeq:" check_it="true" assign_to="cseq"/>
         <add assign_to="cseq" value="9000"/>
     </action>     
  </recv>

</scenario>
