<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE scenario SYSTEM "sipp.dtd">
                                                               --
<scenario name="Basic Sipstone UAC">
  <send retrans="500" dialog="1">
    <![CDATA[

      INVITE sip:[service]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: sipp <sip:sipp@[local_ip]:[local_port]>[local_tag_param]
      To: sut <sip:[service]@[remote_ip]:[remote_port]>
      Call-ID: [call_id]
      CSeq: [cseq] INVITE
      Contact: sip:sipp@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Type: application/sdp
      Content-Length: [len]

      v=0
      o=user1 53655765 2353687637 IN IP[local_ip_type] [local_ip]
      s=-
      c=IN IP[media_ip_type] [media_ip]
      t=0 0
      a=sendrecv
      m=audio [media_port] RTP/AVP 0
      a=rtpmap:0 PCMU/8000

    ]]>
  </send>
 

  <recv response="100"
  		 dialog="1"
        optional="true">
  </recv>

  <recv response="180" optional="true" dialog="1">
  </recv>

  <recv response="183" optional="true" dialog="1">
  </recv>

  <recv response="200" rtd="true" dialog="1">
  </recv>

  <send dialog="1">
    <![CDATA[

      ACK sip:[service]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: sipp <sip:sipp@[local_ip]:[local_port]>[local_tag_param]
      To: sut <sip:[service]@[remote_ip]:[remote_port]>[remote_tag_param]
      Call-ID: [call_id]
      CSeq: [cseq] ACK
      Contact: sip:sipp@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Length: 0

    ]]>
  </send>

  <pause milliseconds="1000"/>

  <send retrans="500" dialog="1">
    <![CDATA[

      BYE sip:[service]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: sipp <sip:sipp@[local_ip]:[local_port]>[local_tag_param]
      To: sut <sip:[service]@[remote_ip]:[remote_port]>[remote_tag_param]
      Call-ID: [call_id]
      CSeq: [cseq] BYE
      Contact: sip:sipp@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Length: 0

    ]]>
  </send>

  <recv response="200" crlf="true" dialog="1">
  </recv>
  
<!-- second call -->

 
    <send retrans="500" dialog="2">
    <![CDATA[

      INVITE sip:[service]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: sipp <sip:sipp@[local_ip]:[local_port]>[local_tag_param]
      To: sut <sip:[service]@[remote_ip]:[remote_port]>
      Call-ID: [call_id]
      CSeq: [cseq] INVITE
      Contact: sip:sipp@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Type: application/sdp
      Content-Length: [len]

      v=0
      o=user1 53655765 2353687637 IN IP[local_ip_type] [local_ip]
      s=-
      c=IN IP[media_ip_type] [media_ip]
      t=0 0
      a=sendrecv
      m=audio [media_port] RTP/AVP 0
      a=rtpmap:0 PCMU/8000

    ]]>
  </send>
  
  <recv response="100"
  		 dialog="2"
        optional="true">
  </recv>

  <recv response="180" optional="true" dialog="2">
  </recv>

  <recv response="183" optional="true" dialog="2">
  </recv>

  <recv response="200" rtd="true" dialog="2">
  </recv>

  <send dialog="2">
    <![CDATA[

      ACK sip:[service]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: sipp <sip:sipp@[local_ip]:[local_port]>[local_tag_param]
      To: sut <sip:[service]@[remote_ip]:[remote_port]>[remote_tag_param]
      Call-ID: [call_id]
      CSeq: [cseq] ACK
      Contact: sip:sipp@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Length: 0

    ]]>
  </send>

  <pause milliseconds="1000"/>

  <send retrans="500" dialog="2">
    <![CDATA[

      BYE sip:[service]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: sipp <sip:sipp@[local_ip]:[local_port]>[local_tag_param]
      To: sut <sip:[service]@[remote_ip]:[remote_port]>[remote_tag_param]
      Call-ID: [call_id]
      CSeq: [cseq] BYE
      Contact: sip:sipp@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Length: 0

    ]]>
  </send>

  <recv response="200" crlf="true" dialog="2">
  </recv>

  <!-- completed 2 calls -->
  
  <send dialog="3">
<![CDATA[
SUBSCRIBE [next_url dialog="1"] SIP/2.0
From: State_Agent line1_to_uri <to@line1.com>;tag=a59ec025
To: line1_from_uri <ugh@line1.com>
CSeq: [cseq] SUBSCRIBE
Call-Id: [call_id]
Event: dialog;sla;include-session-description
